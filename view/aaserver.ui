                <property name="text" translatable="yes">instant powerups</property>
                <property name="variable">string:f_instant</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_quaddrop">
