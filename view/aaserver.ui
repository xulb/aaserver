                  <property name="row">2</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_nohealth">
                <property name="onvalue">NO_HEALTH</property>
                <property name="text" translatable="yes">no health</property>
                <property name="variable">string:f_nohealth</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_weapstay">
                <property name="onvalue">WEAPONS_STAY</property>
                <property name="text" translatable="yes">weapons stay</property>
                <property name="variable">string:f_weapstay</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_infammo">
                <property name="onvalue">INFINITE_AMMO</property>
                <property name="text" translatable="yes">infinite ammo</property>
                <property name="variable">string:f_infammo</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">5</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_noarmor">
                <property name="onvalue">NO_ARMOR</property>
                <property name="text" translatable="yes">no armor</property>
                <property name="variable">string:f_noarmor</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">6</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Labelframe" id="endg_frame">
            <property name="height">65</property>
            <property name="text" translatable="yes">End-Game</property>
            <property name="width">150</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">1</property>
              <property name="sticky">nw</property>
            </layout>
            <child>
              <object class="ttk.Checkbutton" id="chk_samemap">
                <property name="onvalue">SAME_LEVEL</property>
                <property name="text" translatable="yes">same map</property>
                <property name="variable">string:f_samemap</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_allowexit">
                <property name="onvalue">ALLOW_EXIT</property>
                <property name="text" translatable="yes">allow exit</property>
                <property name="variable">string:f_allowexit</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Labelframe" id="others_frame">
            <property name="height">40</property>
            <property name="text" translatable="yes">Other</property>
            <property name="width">150</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">2</property>
              <property name="sticky">nw</property>
            </layout>
            <child>
              <object class="ttk.Checkbutton" id="chk_fixfov">
                <property name="onvalue">FIXED_FOV</property>
                <property name="text" translatable="yes">fixed FOV</property>
                <property name="variable">string:f_fixfov</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="Frame_2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
        <child>
