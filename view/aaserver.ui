<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Notebook" id="aaserver_nbk">
    <property name="height">200</property>
    <property name="width">400</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Notebook.Tab" id="svr_tab">
        <property name="text" translatable="yes">Server</property>
        <child>
          <object class="ttk.Frame" id="svr_tab_frame">
            <property name="height">200</property>
            <property name="width">400</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="ttk.Labelframe" id="svr_frame">
                <property name="height">100</property>
                <property name="text" translatable="yes">Current</property>
                <property name="width">400</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">False</property>
                  <property name="row">0</property>
                  <columns>
                    <column id="0">
                      <property name="minsize">90</property>
                    </column>
                  </columns>
                </layout>
                <child>
                  <object class="tk.Message" id="msg_mapname">
                    <property name="text" translatable="yes">                      </property>
                    <property name="textvariable">string:current_map</property>
                    <property name="width">300</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="columnspan">1</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_curmap">
                    <property name="text" translatable="yes">Current map:</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_addr">
                    <property name="text" translatable="yes">Address:</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_svr">
                    <property name="text" translatable="yes">Server:</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Message" id="msg_servername">
                    <property name="textvariable">string:server_name</property>
                    <property name="width">250</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Message" id="msg_svraddr">
                    <property name="textvariable">string:svr_addr</property>
                    <property name="width">150</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="btn_refresh">
                    <property name="command">on_refresh_click</property>
                    <property name="text" translatable="yes">Refresh</property>
                    <layout>
                      <property name="column">2</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                      <property name="sticky">ne</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Labelframe" id="cntl_frame">
                <property name="height">100</property>
                <property name="text" translatable="yes">Controls</property>
                <property name="width">400</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">False</property>
                  <property name="row">1</property>
                  <columns>
                    <column id="0">
                      <property name="minsize">42</property>
                    </column>
                  </columns>
                </layout>
                <child>
                  <object class="ttk.Entry" id="txt_rcon_cmd">
                    <property name="textvariable">string:rcon_cmd</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">5</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="btn_start_map">
                    <property name="command">on_start_map_click</property>
                    <property name="text" translatable="yes">Start map</property>
                    <layout>
                      <property name="column">2</property>
                      <property name="padx">20</property>
                      <property name="propagate">True</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Combobox" id="cmb_maps">
                    <property name="textvariable">string:selected_map</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_newmap">
                    <property name="text" translatable="yes">New map:</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">3</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="btn_send_cmd">
                    <property name="command">on_send_cmd_click</property>
                    <property name="text" translatable="yes">Send</property>
                    <layout>
                      <property name="column">2</property>
                      <property name="propagate">True</property>
                      <property name="row">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_cmd">
                    <property name="text" translatable="yes">Command</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">5</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Notebook.Tab" id="plyr_tab">
        <property name="text" translatable="yes">Players</property>
        <child>
          <object class="ttk.Frame" id="plyr_tab_frame">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Menu" id="menu_main">
    <property name="tearoff">true</property>
    <child>
      <object class="tk.Menuitem.Command" id="menu_item_server">
        <property name="command">on_menu_item_server_click</property>
        <property name="command_id_arg">false</property>
        <property name="font">TkDefaultFont</property>
        <property name="label" translatable="yes">Server</property>
        <property name="state">normal</property>
      </object>
    </child>
  </object>
  <object class="pygubu.builder.widgets.dialog" id="dialog_open_server">
    <property name="height">100</property>
    <property name="modal">true</property>
    <property name="padx">25</property>
    <property name="pady">15</property>
    <property name="resizable">none</property>
    <property name="takefocus">true</property>
    <property name="title" translatable="yes">Open Server</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Label" id="lbl_svr_addr">
        <property name="text" translatable="yes">Address:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="open_svr_addr">
        <property name="textvariable">string:open_svr_addr</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lbl_svr_pass">
        <property name="text" translatable="yes">Rcon pass:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="open_svr_pass">
        <property name="textvariable">string:open_svr_pass</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btn_connect">
        <property name="command">on_svr_dlg_connect_click</property>
        <property name="default">normal</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Connect</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
  </object>
  <object class="pygubu.builder.widgets.dialog" id="dialog_dmflags">
    <property name="height">100</property>
    <property name="modal">false</property>
    <property name="width">300</property>
    <child>
      <object class="ttk.Frame" id="Frame_1">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
        <child>
          <object class="ttk.Labelframe" id="pu_frame">
            <property name="height">160</property>
            <property name="text" translatable="yes">Pick-ups</property>
            <property name="width">150</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
              <property name="sticky">nw</property>
            </layout>
            <child>
              <object class="ttk.Checkbutton" id="chk_nopwr">
                <property name="onvalue">NO_ITEMS</property>
                <property name="text" translatable="yes">no powerups</property>
                <property name="variable">string:f_nopwr</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_instant">
