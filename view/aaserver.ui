                  <property name="row">1</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="chk_teamsskin">
                <property name="onvalue">SKINTEAMS</property>
                <property name="text" translatable="yes">teams by skin</property>
                <property name="variable">string:f_teamskin</property>
                <layout>
